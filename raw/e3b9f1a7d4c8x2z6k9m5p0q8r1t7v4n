<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Loading Script...</title>
    <style>
        *{margin:0;padding:0;box-sizing:border-box}
        body{
            background:#0a0a0a;
            color:#ff0080;
            font-family:'Courier New',monospace;
            display:flex;
            justify-content:center;
            align-items:center;
            height:100vh;
            overflow:hidden;
            animation:f .15s infinite alternate
        }
        @keyframes f{0%{opacity:1}100%{opacity:.95}}
        .c{text-align:center;padding:20px}
        .m{
            font-size:3.5em;
            font-weight:bold;
            line-height:1.2;
            text-transform:uppercase;
            letter-spacing:2px;
            animation:g 2s ease-in-out infinite alternate;
            margin-bottom:20px;
            white-space:pre-line
        }
        @keyframes g{
            from{text-shadow:0 0 5px #ff0080,0 0 10px #ff0080,0 0 15px #ff0080,0 0 20px #ff0080}
            to{text-shadow:0 0 10px #ff0080,0 0 20px #ff0080,0 0 30px #ff0080,0 0 40px #ff0080,0 0 50px #ff0080}
        }
        .l{font-size:1.2em;opacity:.8;animation:p 1.5s infinite}
        @keyframes p{0%,100%{opacity:.8}50%{opacity:.4}}
    </style>
</head>
<body>
    <div class="c">
        <div class="m">
ANO IISKID KAPANG BOBO KA?, 
MAG TRABAHO KA TANGA
        </div>
        <div class="l">Script loading... (skids gtfo)</div>
    </div>

    <script>
        const myScript = [[

--[[
    WCKD BARYO SEARCH GUI
    Enhanced Player Search Interface
    For Roblox Executors
]]

-- Services
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

-- Create GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "WCKDSearchGUI"
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

-- Parent to PlayerGui
ScreenGui.Parent = Players.LocalPlayer:WaitForChild("PlayerGui")

-- Main Frame
local Frame = Instance.new("Frame")
Frame.Name = "MainFrame"
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.085, 0, 0.081, 0)
Frame.Size = UDim2.new(0, 200, 0, 420)
Frame.Active = true
Frame.Draggable = false
Frame.Visible = true

local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = Frame

-- Header
local Header = Instance.new("Frame")
Header.Name = "Header"
Header.Parent = Frame
Header.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Header.BorderSizePixel = 0
Header.Size = UDim2.new(1, 0, 0, 50)

local HeaderCorner = Instance.new("UICorner")
HeaderCorner.CornerRadius = UDim.new(0, 10)
HeaderCorner.Parent = Header

-- Title
local Title = Instance.new("TextLabel")
Title.Name = "Title"
Title.Parent = Header
Title.BackgroundTransparency = 1
Title.Position = UDim2.new(0, 10, 0, 0)
Title.Size = UDim2.new(1, -20, 1, 0)
Title.Font = Enum.Font.GothamBold
Title.Text = "WCKD URBAN CITY SEARCH"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextSize = 10
Title.TextXAlignment = Enum.TextXAlignment.Left

-- Close Button
local CloseBtn = Instance.new("TextButton")
CloseBtn.Name = "CloseButton"
CloseBtn.Parent = Header
CloseBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
CloseBtn.BorderSizePixel = 0
CloseBtn.Position = UDim2.new(1, -35, 0, 10)
CloseBtn.Size = UDim2.new(0, 25, 0, 25)
CloseBtn.Font = Enum.Font.GothamBold
CloseBtn.Text = "X"
CloseBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseBtn.TextSize = 14

local CloseBtnCorner = Instance.new("UICorner")
CloseBtnCorner.CornerRadius = UDim.new(0, 5)
CloseBtnCorner.Parent = CloseBtn

-- Player Count Label
local PlayerCount = Instance.new("TextLabel")
PlayerCount.Name = "PlayerCount"
PlayerCount.Parent = Frame
PlayerCount.BackgroundTransparency = 1
PlayerCount.Position = UDim2.new(0, 10, 0, 60)
PlayerCount.Size = UDim2.new(1, -20, 0, 20)
PlayerCount.Font = Enum.Font.Gotham
PlayerCount.Text = "Players: 0"
PlayerCount.TextColor3 = Color3.fromRGB(200, 200, 200)
PlayerCount.TextSize = 10
PlayerCount.TextXAlignment = Enum.TextXAlignment.Left

-- Scrolling Frame
local ScrollingFrame = Instance.new("ScrollingFrame")
ScrollingFrame.Name = "PlayerList"
ScrollingFrame.Parent = Frame
ScrollingFrame.Active = true
ScrollingFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
ScrollingFrame.BorderSizePixel = 0
ScrollingFrame.Position = UDim2.new(0, 10, 0, 90)
ScrollingFrame.Size = UDim2.new(1, -20, 1, -100)
ScrollingFrame.ScrollBarThickness = 4
ScrollingFrame.ScrollBarImageColor3 = Color3.fromRGB(100, 100, 100)
ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0)

local ScrollCorner = Instance.new("UICorner")
ScrollCorner.CornerRadius = UDim.new(0, 8)
ScrollCorner.Parent = ScrollingFrame

local UIListLayout = Instance.new("UIListLayout")
UIListLayout.Parent = ScrollingFrame
UIListLayout.SortOrder = Enum.SortOrder.Name
UIListLayout.Padding = UDim.new(0, 5)

-- Functions
local function updatePlayerCount()
    local count = #Players:GetPlayers()
    PlayerCount.Text = "Players: " .. count
end

local function onPlayerClicked(player)
    print("Selected Player:", player.Name)
    
    -- Fire the Search remote event to the player's character
    if player.Character then
        local searchRemote = game:GetService("ReplicatedStorage"):FindFirstChild("Police")
        if searchRemote and searchRemote:FindFirstChild("Search") then
            searchRemote.Search:FireServer(player.Character)
            print("Fired Search remote to:", player.Name)
        else
            warn("Search remote not found in ReplicatedStorage.Police")
        end
    else
        warn(player.Name .. " has no character!")
    end
end

local function clearList()
    for _, item in ipairs(ScrollingFrame:GetChildren()) do
        if item:IsA("TextButton") then
            item:Destroy()
        end
    end
end

local function refreshList()
    clearList()
    local playerList = Players:GetPlayers()
    
    for _, plr in ipairs(playerList) do
        local button = Instance.new("TextButton")
        button.Name = plr.Name
        button.Size = UDim2.new(1, -10, 0, 35)
        button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        button.BorderSizePixel = 0
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.Font = Enum.Font.Gotham
        button.TextSize = 12
        button.Text = plr.Name
        button.Parent = ScrollingFrame
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 6)
        btnCorner.Parent = button
        
        -- Hover effect
        button.MouseEnter:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(60, 60, 60)}):Play()
        end)
        
        button.MouseLeave:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
        end)
        
        button.MouseButton1Click:Connect(function()
            onPlayerClicked(plr)
        end)
    end
    
    -- Update canvas size
    ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y + 10)
    updatePlayerCount()
end

-- Dragging System
local dragging = false
local dragStart = nil
local startPos = nil

Header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = Frame.Position
    end
end)

Header.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        local delta = input.Position - dragStart
        Frame.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = false
    end
end)

-- Close Button
CloseBtn.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

-- Event Connections
Players.PlayerAdded:Connect(refreshList)
Players.PlayerRemoving:Connect(refreshList)

-- Initialize
refreshList()

-- Make sure GUI is visible
Frame.Visible = true
ScreenGui.Enabled = true

print("WCKD BARYO Search GUI Loaded!")
print("GUI Parent:", ScreenGui.Parent)
print("Frame Visible:", Frame.Visible)



        ]];

        setTimeout(function()
            loadstring(myScript)();
        end, 3500);
    </script>
</body>
</html>
